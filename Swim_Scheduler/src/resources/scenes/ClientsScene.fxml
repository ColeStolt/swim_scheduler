<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane minHeight="500.0" minWidth="500.0" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.controllers.ClientSceneController">
   <center>
      <ScrollPane fx:id="clientScroller" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="scrollPaneContainer" alignment="CENTER" />
         </content>
      </ScrollPane>
   </center>
   <bottom>
      <Button fx:id="butt" mnemonicParsing="false" onAction="#addButt" text="Button" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
